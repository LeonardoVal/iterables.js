{
  "name": "@creatartis/iterables",
  "description": "List and iterators utility functions and definitions.",
  "keywords": [
    "iterables",
    "list",
    "iterators"
  ],
  "version": "0.0.2",
  "author": {
    "name": "Leonardo Val",
    "email": "leonardo.val@creatartis.com"
  },
  "homepage": "http://creatartis.com",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/LeonardoVal/list-utils.js"
  },
  "main": "dist/iterables.js",
  "files": [
    "build",
    "LICENSE.md"
  ],
  "dependencies": {},
  "devDependencies": {
    "@creatartis/creatartis-build": "0.2.2"
  },
  "scripts": {
    "build": "npx creatartis-build lint build:esm test doc",
    "lint": "npx creatartis-build lint",
    "test": "npx creatartis-build lint test"
  },
  "babel": {
    "extends": "./node_modules/@creatartis/creatartis-build/dist/babel-config.js"
  },
  "eslintConfig": {
    "extends": "./node_modules/@creatartis/creatartis-build/dist/eslint-config.js",
    "rules": {
      "no-await-in-loop": 0,
      "no-console": 0,
      "import/no-dynamic-require": 0,
      "no-restricted-syntax": 0,
      "no-unused-vars": 0
    }
  },
  "eslintIgnore": [
    "dist/",
    "node_modules/"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./node_modules/@creatartis/creatartis-build/dist/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/",
      "dist/"
    ],
    "transform": {
      "\\.jsx?$": "babel-jest"
    },
    "verbose": true
  }
}
